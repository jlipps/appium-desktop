{
  "parser": "babel-eslint",
  "extends": [
    "appium"
  ],
  "plugins": [
    "import",
    "react",
    "react-native"
  ],
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "import/no-unresolved": [2, {"ignore": ["electron"]}],
    "babel/arrow-parens": 0,
    "require-await": 2,
    "react-native/no-inline-styles": 2
  }
}
